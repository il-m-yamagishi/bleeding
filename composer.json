{
    "name": "il-m-yamagishi/bleeding",
    "description": "Bleeding Edge PHP 8 Framework",
    "keywords": ["php8", "framework"],
    "license": "Apache 2.0",
    "homepage": "https://github.com/il-m-yamagishi/bleeding",
    "support": {
        "issues": "https://github.com/il-m-yamagishi/bleeding/issues",
        "source": "https://github.com/il-m-yamagishi/bleeding"
    },
    "authors": [
        {
            "name": "Masaru Yamagishi",
            "email": "m-yamagishi@infiniteloop.co.jp",
            "homepage": "https://cafe-capy.net",
            "role": "Author"
        }
    ],
    "require": {
        "php": "~8.0-dev",
        "ext-json": "*",
        "guzzlehttp/psr7": "^1.6",
        "laminas/laminas-diactoros": "^2.4",
        "monolog/monolog": "^2.1",
        "narrowspark/http-emitter": "^1.0",
        "php-di/php-di": "^6.2",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "relay/relay": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Bleeding\\": "Bleeding/",
            "Edge\\": "Edge/"
        },
        "files": [
            "Bleeding/Entrypoint/makeContainer.php",
            "Bleeding/Entrypoint/makeResolver.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
